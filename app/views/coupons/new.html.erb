<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Coupon Form</title>
  </head>
  <body>
    <%= form_tag coupons_path %>
    <h3> Coupon Code: </h3>
    <%= text_field_tag :coupon_code %>
    <h3> Coupon Store: </h3>
    <%= text_field_tag :store %>
    <%= submit_tag "Submit Coupon" %>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  </body>
</html>
